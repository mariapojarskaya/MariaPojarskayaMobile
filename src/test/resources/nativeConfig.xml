<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite name="Mobile test suite" verbose="1" >
    <test name="Native tests">
        <groups>
            <run>
                <exclude name="web" />
                <include name="native"/>
            </run>
        </groups>
        <packages>
            <package name="ex3.scenarios.*"/>
        </packages>
    </test>
</suite>
